{
  "name": "my-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel css:watch serve",
    "serve": "eleventy --serve",
    "css:watch": "postcss src/assets/css/input.css -o docs/assets/css/tailwind.css --watch",
    "build": "eleventy",
    "css:build": "postcss src/assets/css/input.css -o docs/assets/css/tailwind.css --no-map"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "alpinejs": "^3.12.0",
    "autoprefixer": "^10.4.0",
    "cytoscape": "^3.24.0",
    "markdown-it": "^13.0.0",
    "markdown-it-anchor": "^8.4.0",
    "markdown-it-regexp": "^0.4.0",
    "markdown-it-table-of-contents": "^0.4.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.0",
    "postcss-cli": "^9.1.0",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.0"
  }
}
